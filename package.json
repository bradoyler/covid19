{
  "name": "covid19",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "publish": " cd dist && s3-upload",
    "deploy": "npm run deploy:assets && npm run deploy:index",
    "deploy:assets": "s3-deploy './dist/**/*.{js,css,png,svg,csv}' --cwd './dist/' --region us-east-1 --bucket covid.thenewsdivide.com --gzip js,css,svg,txt,csv --cache 31556952 --preventUpdates",
    "deploy:index": "s3-deploy './dist/index.html' --cwd './dist/' --region us-east-1 --bucket covid.thenewsdivide.com --noCache"

  },
  "dependencies": {
    "@turf/turf": "^5.1.6",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "d3-dsv": "^1.2.0",
    "s3-deploy": "^1.4.0",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.0",
    "@vue/cli-plugin-eslint": "^4.3.0",
    "@vue/cli-service": "^4.3.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  }
}
